  },
  "main": "app/app.js",
  "dependencies": {
    "angular": "^1.7.5",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "ngGeolocation": "0.0.7",
    "popper.js": "^1.14.4"
  },
  "devDependencies": {
    "http-server": "^0.11.1"
  },
  "scripts": {
    "prestart": "npm install",
